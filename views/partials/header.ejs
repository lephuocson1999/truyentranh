                                                                                                            if (true == data.success) {
                                                                                                                return {
                                                                                                                    label: item.title,
                                                                                                                    value: item.title,
                                                                                                                    url: item.url,
                                                                                                                    type: item.type,
                                                                                                                };
                                                                                                            } else {
                                                                                                                return {
                                                                                                                    label: item.message,
                                                                                                                    value: item.message,
                                                                                                                    type: item.type,
                                                                                                                    click: false,
                                                                                                                };
                                                                                                            }
                                                                                                        })
                                                                                                    );
                                                                                                },
                                                                                            });
                                                                                        },
                                                                                        select: function (e, ui) {
                                                                                            if (ui.item.url) {
                                                                                                window.location.href = ui.item.url;
                                                                                            } else {
                                                                                                if (ui.item.click) {
                                                                                                    return true;
                                                                                                } else {
                                                                                                    return false;
                                                                                                }
                                                                                            }
                                                                                        },
                                                                                        open: function (e, ui) {
                                                                                            var acData = $(this).data("uiAutocomplete");
                                                                                            acData.menu.element
                                                                                                .addClass("manga-autocomplete")
                                                                                                .find("li div")
                                                                                                .each(function () {
                                                                                                    var $self = $(this),
                                                                                                        keyword = $.trim(acData.term).split(" ").join("|");
